# Authentication Settings
# Set to false in production to enable proper authentication
BYPASS_AUTH=false
API_TOKEN=dev-token
DEFAULT_ROLE=admin

# Database
# DATABASE_URL=your_database_url_here
# SUPABASE_URL=your_supabase_url_here

# Cloudinary (optional - for image uploads)
# CLOUDINARY_CLOUD_NAME=
# CLOUDINARY_API_KEY=
# CLOUDINARY_API_SECRET=

# VAPID Keys for Push Notifications (optional)
# VAPID_PUBLIC_KEY=
# VAPID_PRIVATE_KEY=

# Server Settings
PORT=5000
NODE_ENV=development

# Redirect Settings (optional)
# REDIRECT_ENABLED=false
# REDIRECT_FROM_HOSTS=old-domain.com
# REDIRECT_TARGET_HOST=new-domain.com
