# Example Environment Configuration
# Copy this file to .env.development or .env.production and update the values

# Django Backend API URL
# Development: http://localhost:8000
# Production: https://rotc-django-web.onrender.com (Django Web Service)
VITE_API_URL=http://localhost:8000

# WebSocket URL for real-time updates (Django Channels)
# Development: ws://localhost:8000
# Production: wss://rotc-django-channels.onrender.com (Django Channels Service)
VITE_WS_URL=ws://localhost:8000

# Environment (development or production)
VITE_ENV=development
